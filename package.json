{
  "name": "atn-utilities",
  "version": "1.0.0",
  "description": "Utility bot for ATN Server",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm run delete && npm run compile",
    "compile": "tsc",
    "test": "eslint && tsc --noEmit && npm run prettier",
    "delete": "rm -rf ./dist",
    "start": "npm run build && node --enable-source-maps .",
    "lint": "eslint src --fix",
    "prepare": "npm test",
    "prettier": "prettier --write src/**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NotReallyEight/atn-utilities.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/NotReallyEight/atn-utilities/issues"
  },
  "homepage": "https://github.com/NotReallyEight/atn-utilities#readme",
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "chalk": "^4.1.2",
    "dotenv": "^16.0.0",
    "eris": "^0.16.1",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@tsconfig/node16": "^1.0.2",
    "@types/node": "^16.11.26",
    "eslint": "^8.10.0",
    "eslint-plugin-node": "^11.1.0",
    "prettier": "^2.5.1",
    "typescript": "^4.5.5"
  },
  "engines": {
    "node": ">=16.14.0"
  }
}
